{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:best-practices", ":dependencyDashboard"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "rangeStrategy": "pin",
      "automerge": true,
      "automergeType": "pr",
      "automergeStrategy": "merge-commit",
      "matchDepTypes": ["*"]
    }
  ],
  "platformAutomerge": true
}
